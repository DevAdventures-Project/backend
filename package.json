{
  "name": "backend",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "pnpm prisma generate && nest build",
    "start": "pnpm prisma generate && nest start",
    "dev": "pnpm prisma generate && nest start --watch",
    "start:debug": "pnpm prisma generate && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome check --write --unsafe",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prisma:deploy": "pnpm prisma migrate deploy"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/platform-socket.io": "^11.0.11",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/websockets": "^11.0.11",
    "@prisma/client": "^6.4.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.7",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.13.9",
    "@types/passport-jwt": "^4.0.1",
    "prisma": "^6.4.1",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm@10.5.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@nestjs/core",
      "@prisma/client",
      "@prisma/engines",
      "@scarf/scarf",
      "@swc/core",
      "bcrypt",
      "esbuild",
      "prisma"
    ]
  },
  "engines": {
    "node": ">=20"
  }
}
